<h1>Reservation form </h1>
<%= form_for listing_reservations_path do |f|%>
    <%=f.label :guest_amount%><br>
    <%=f.number_field :guest_amount %><br>

    <%=f.label :checkin%><br>
    <%=f.date_field  :checkin %><br>

    <%=f.label :checkout%><br>
    <%=f.date_field  :checkout %><br>
   <%=f.submit "Make Reservation"%>
    <%end%>
